[alias]
    amend = commit -a --amend
    br = branch
    cl = clean -fd
    cm = git commit -m
    co = checkout
    cob = checkout -b
    dlbr = branch -d 
    drbr = push -d orgin
    ec = config --global -e
    fod = fetch origin develop
    lg = log --graph
    lo = log --pretty=oneline
    logwho = log --no-merges
    poadu = push origin origin develop
    pod = pull origin develop
    rl = checkout .
    rmu = clean -d -f
    save = !git add -A && git commit -m 'SAVEPOINT'
    st = status     
    undo = reset HEAD~1 --mixed
    up = !git pull --rebase --prune $@ && git submodule update --init --recursive
    what = whatchanged
    wip = commit -am "WIP"
    wipe = reset --hard HEAD
